<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/toolbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/text_handle_list"
            style="@style/TextTab"
            android:drawableTop="@drawable/icon_delivery"
            android:text="理库任务" />

        <TextView
            android:id="@+id/text_handle_num"
            style="@style/TextTab"
            android:drawableTop="@drawable/icon_back_goos_out"
            android:text="理库点数" />


        <TextView
            android:id="@+id/text_handle_manual"
            style="@style/TextTab"
            android:drawableTop="@drawable/icon_goods_stock"
            android:text="手动理库" />

    </LinearLayout>


</LinearLayout>